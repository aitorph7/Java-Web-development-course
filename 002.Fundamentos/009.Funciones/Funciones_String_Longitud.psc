Algoritmo Funciones_String
	
	// Calcula la longitud de un texto 
	textLength <- Longitud("Hola mundo") // 10
	
	Escribir "La longitud de Hola mundo es: ", textLength
	
	Escribir "Introduce a phrase: "
	Leer phrase
	phraseLength <- Longitud(phrase)
	Escribir "La longitud de ", phrase, " es " phraseLength
	
FinAlgoritmo
